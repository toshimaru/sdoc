<ul style="<%= nested ? "display:none;" : "navigation" %>">
<% tree.each do |class_name, path, file_name, nested_tree| %>
<li>
  <% if nested_tree.any? %><div class="icon"></div><% end %>
  <% if path.empty? %>
    <span class="text-muted"><%= file_name.empty? ? class_name : file_name %></span>
  <% else %>
    <a href="<%= "/#{path}" %>"><%= class_name %></a>
  <% end %>
  <% if nested_tree.any? %><%= include_template '_navigation_tree.html.erb', { tree: nested_tree, nested: true } %><% end %>
</li>
<% end %>
</ul>
