<ul style="<%= nested ? "display:none;" : "" %>">
  <% tree.each do |class_name, path, file_name, nested_tree| %>
    <li>
      <% if nested_tree.any? %>
        <div class="icon"></div>
      <% end %>
      <% if path.empty? %>
        <%= file_name %>
      <% else %>
        <a href="<%= "#{rel_prefix}/#{path}" %>"><%= class_name %></a>
      <% end %>

      <% if nested_tree.any? %>
        <%= include_template '_navigation_tree.html.erb', { tree: nested_tree, rel_prefix: rel_prefix, nested: true } %>
      <% end %>
    </li>
  <% end %>
</ul>
